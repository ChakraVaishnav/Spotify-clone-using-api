# Spotify API Configuration
# Copy this file to .env and fill in your values

# Your Spotify Client ID (get from https://developer.spotify.com/dashboard)
SPOTIFY_CLIENT_ID=your_client_id_here

# Environment: development or production
NODE_ENV=development

# Redirect URIs (will auto-detect based on current URL if not set)
# Development URL (optional - defaults to http://localhost:5500)
DEV_REDIRECT_URI=http://localhost:5500/

# Production URL (optional - only needed if you specify production URIs)
PROD_REDIRECT_URI=https://your-production-domain.vercel.app/
